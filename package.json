{
  "name": "only-pizza-burger-or-sushi",
  "version": "0.1.0",
  "private": true,
  "author": "Jo√£o Guilherme <hi@guicheffer.me>",
  "homepage": "http://guicheffer.me/",
  "repository": {
    "type": "git",
    "url": "https://github.com/guicheffer/only-pizza-burger-or-sushi/"
  },
  "var": {
    "dist": "./build/",
    "port": "3000",
    "runner": "react-scripts",
    "server": "http-server"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.2.5",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "12.0.4",
    "@types/jest": "24.0.0",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "7.1.7",
    "http-server": "0.12.3",
    "react": "16.13.1",
    "react-dom": "16.13.0",
    "react-redux": "7.2.0",
    "react-scripts": "3.4.0",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "typescript": "4.0.3"
  },
  "devDependencies": {
    "@testing-library/dom": "7.26.0",
    "@types/redux-mock-store": "1.0.2",
    "@types/reselect": "2.2.0",
    "node-sass": "4.14.1",
    "redux-mock-store": "1.5.4"
  },
  "engineStrict": true,
  "engines": {
    "node": "14.*",
    "yarn": "1.*"
  },
  "scripts": {
    "start": "$npm_package_var_runner start",
    "build": "$npm_package_var_runner build",
    "test": "$npm_package_var_runner test --no-cache",
    "test:coverage": "$npm_package_var_runner test --watchAll=false --update-snapshot --no-cache --collect-coverage",
    "serve": "$npm_package_var_server $npm_package_var_dist -p $npm_package_var_port"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
